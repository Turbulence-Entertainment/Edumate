<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".student_details">

    <ImageView
        android:id="@+id/name_details4"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_details3"
        app:srcCompat="@drawable/enter_block" />

    <ImageView
        android:id="@+id/name_details5"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_details4"
        app:srcCompat="@drawable/enter_block" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="25dp"
        android:text="Welcome to"
        android:textColor="#FF0101"
        android:textSize="35sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edumate"
        android:textColor="#FFFFFF"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView23"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

    <TextView
        android:id="@+id/profession_title_details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageView
        android:id="@+id/lets_rock_stu"
        android:layout_width="150dp"
        android:layout_height="51dp"
        android:onClick="submit"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        app:srcCompat="@drawable/button" />

    <ImageView
        android:id="@+id/name_details"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profession_title_details"
        app:srcCompat="@drawable/enter_block" />

    <ImageView
        android:id="@+id/name_details6"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_details5"
        app:srcCompat="@drawable/enter_block" />

    <ImageView
        android:id="@+id/name_details3"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_details2"
        app:srcCompat="@drawable/enter_block" />

    <ImageView
        android:id="@+id/name_details2"
        android:layout_width="290dp"
        android:layout_height="48dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_details"
        app:srcCompat="@drawable/enter_block" />

    <EditText
        android:id="@+id/name"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:visibility="gone"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <TextView
        android:id="@+id/name_view"
        android:layout_width="260dp"
        android:layout_height="34dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintStart_toStartOf="@+id/name_details" />


    <EditText
        android:id="@+id/payment"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:visibility="gone"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details6"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <TextView
        android:id="@+id/payment_view"
        android:layout_width="260dp"
        android:layout_height="34dp"
        android:ems="10"
        android:hint="School/College/University name"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details6"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <EditText
        android:id="@+id/mail"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/name_details5"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <TextView
        android:id="@+id/mail_view"
        android:layout_width="260dp"
        android:layout_height="34dp"
        android:ems="10"
        android:hint="Email Address"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details5"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <EditText
        android:id="@+id/address"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/name_details4"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <TextView
        android:id="@+id/address_view"
        android:layout_width="260dp"
        android:layout_height="34dp"
        android:ems="10"
        android:hint="Address"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details4"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />


    <EditText
        android:id="@+id/dob"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:visibility="gone"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details3"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <TextView
        android:id="@+id/dob_view"
        android:layout_width="260dp"
        android:layout_height="34dp"
        android:ems="10"
        android:hint="Date Of Birth"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_details3"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/name_details" />

    <EditText
        android:id="@+id/phone"
        android:layout_width="260dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/name_details3"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintStart_toStartOf="@+id/name_details"
        app:layout_constraintTop_toTopOf="@+id/name_details2" />

    <TextView
        android:id="@+id/phone_view"
        android:layout_width="260dp"
        android:layout_height="34dp"
        android:ems="10"
        android:hint="Mobile Number"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/name_details3"
        app:layout_constraintEnd_toEndOf="@+id/name_details"
        app:layout_constraintStart_toStartOf="@+id/name_details"
        app:layout_constraintTop_toTopOf="@+id/name_details2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Select Your Gender"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/name_details6"
        app:layout_constraintTop_toBottomOf="@+id/name_details6" />

    <CheckBox
        android:id="@+id/m"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Male"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <CheckBox
        android:id="@+id/f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Female"
        app:layout_constraintStart_toEndOf="@+id/m"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <CheckBox
        android:id="@+id/o"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other"
        app:layout_constraintStart_toStartOf="@+id/m"
        app:layout_constraintTop_toBottomOf="@+id/m" />

    <TextView
        android:id="@+id/lets_rock_text_stu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Let's Go"
        android:textColor="#000000"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/lets_rock_stu"
        app:layout_constraintEnd_toEndOf="@+id/lets_rock_stu"
        app:layout_constraintStart_toStartOf="@+id/lets_rock_stu"
        app:layout_constraintTop_toTopOf="@+id/lets_rock_stu" />

    <ProgressBar
        android:id="@+id/lets_rock_progresbar_stu"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/lets_rock_stu"
        app:layout_constraintEnd_toEndOf="@+id/lets_rock_stu"
        app:layout_constraintStart_toStartOf="@+id/lets_rock_stu"
        app:layout_constraintTop_toTopOf="@+id/lets_rock_stu" />

</androidx.constraintlayout.widget.ConstraintLayout>